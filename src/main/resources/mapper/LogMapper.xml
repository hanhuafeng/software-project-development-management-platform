      select l.*,u.true_name from project p
      LEFT JOIN relation_project_to_log rptl
      on p.id = rptl.project_id
      LEFT JOIN log l
      on l.id = rptl.log_id
      left join relation_user_to_log rutl
      on rutl.log_id = l.id
      left join user u
      on u.id = rutl.user_id
      where p.id = #{projectId,jdbcType=INTEGER}
      <if test="trueName!=null">
        and true_name like "%"#{trueName,jdbcType=VARCHAR}"%"
